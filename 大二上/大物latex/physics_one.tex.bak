\[\begin{gathered}
  {P_i} = P({Y_i} = 1|{x_i}) = E({Y_i}|{x_i}) = \frac{1}{{1 + {e^{ - z}}}} \hfill \\
  z = {\beta _1} + {\beta _2}{x_i} \hfill \\
   \Rightarrow {P_i} = \frac{1}{{1 + {e^{ - ({\beta _1} + {\beta _2}{x_i})}}}} \hfill \\
  {\text{当z从 - }}\infty {\text{ 变化到  + }}\infty ,{p_i}{\text{ 从 0 变化到 1,引用logit模型的优势比率(odds),
  界定事件发生或不发生的比中“违约”为优势 }} \hfill \\
  {P_i} = \frac{1}{{1 + {e^{ - ({\beta _1} + {\beta _2}{x_i})}}}} \hfill \\
  1 - {P_i} = \frac{1}{{1 + {e^{({\beta _1} + {\beta _2}{x_i})}}}} \hfill \\
  B = \frac{{{P_i}}}{{1 - {P_i}}} = {e^{({\beta _1} + {\beta _2}{x_i})}} \hfill \\
  {\text{即引出logit公式 }} \hfill \\
  H = \ln (B) = \ln (\frac{{{P_i}}}{{1 - {P_i}}}) = {\beta _1} + {\beta _2}{x_i} + {u_i} \hfill \\
\end{gathered} \]
